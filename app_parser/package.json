{
  "name": "app_parser",
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -rf lib",
    "clean:build": "npm run clean && npm run build",
    "test": "npm run build && npm run test:coverage && npm run test:lint",
    "test:ava": "npx -n \"$NODE_DEBUG_OPTION\" ava",
    "test:watch": "npm run test:ava -- --watch",
    "test:coverage": "nyc npm run test:ava",
    "test:lint": "eslint --ext .ts src/",
    "test:lint:fix": "npm run test:lint -- --fix",
    "util:esw": "esw --ext .ts src/",
    "util:esw:watch": "npm run util:esw -- -w",
    "util:nibble": "eslint-nibble --ext .ts src/",
    "bin": "npm run build && node $NODE_DEBUG_OPTION lib/bin",
    "start": "npm run build && time node $NODE_DEBUG_OPTION lib/index.js",
    "reanalyseLibs": "npm run build && node $NODE_DEBUG_OPTION lib/dumpWatch/reanalyser",
    "parseJSON": "npm run build && time node $NODE_DEBUG_OPTION lib/parseJSON.js",
    "watch:lib_dump": "npm run build && node $NODE_DEBUG_OPTION lib/dumpWatch"
  },
  "dependencies": {
    "@types/babel-types": "^6.25.2",
    "@types/babylon": "^6.16.2",
    "@types/chokidar": "^1.7.5",
    "@types/common-tags": "^1.4.0",
    "@types/debug": "0.0.30",
    "@types/fs-extra": "^5.0.1",
    "@types/generic-pool": "^3.1.2",
    "@types/jsdom": "^11.0.4",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.6.1",
    "@types/request": "^2.47.0",
    "@types/request-promise-native": "^1.0.14",
    "@types/shelljs": "^0.7.8",
    "@types/tar": "^4.0.0",
    "@types/uuid": "^3.4.3",
    "@types/yargs": "^11.0.0",
    "@types/zen-observable": "^0.5.3",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0",
    "chokidar": "^2.0.3",
    "common-tags": "^1.7.2",
    "debug": "^3.1.0",
    "extract-comments": "^1.0.0",
    "freshy": "^1.0.4",
    "fs-extra": "^5.0.0",
    "generic-pool": "^3.4.2",
    "jsdom": "^11.6.2",
    "left-pad": "^1.2.0",
    "lodash": "^4.17.5",
    "proper-lockfile": "^3.0.2",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "shelljs-exec-proxy": "^0.1.1",
    "tar": "^4.4.1",
    "typescript": "^2.8.1",
    "typical-ts": "^0.2.1",
    "uuid": "^3.2.1",
    "workerpool": "^2.3.0",
    "yargs": "^11.0.0",
    "zen-observable": "^0.8.8"
  },
  "devDependencies": {
    "@types/sinon": "^4.3.0",
    "ava": "^0.25.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-import-resolver-typescript": "^1.0.2",
    "eslint-nibble": "^4.2.1",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-typescript": "^0.11.0",
    "eslint-plugin-unicorn": "^4.0.2",
    "eslint-watch": "^3.1.4",
    "nyc": "^11.6.0",
    "prettier": "^1.11.1",
    "sinon": "^4.5.0",
    "typescript-eslint-parser": "^14.0.0"
  },
  "ava": {
    "files": [
      "lib/**/*.test.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "snapshotDir": "test/snapshots",
    "verbose": true
  },
  "nyc": {
    "all": true,
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "parser": "typescript-eslint-parser",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "plugins": [
      "ava",
      "import",
      "prettier",
      "promise",
      "typescript",
      "unicorn"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:ava/recommended",
      "plugin:import/recommended",
      "plugin:prettier/recommended",
      "plugin:promise/recommended",
      "plugin:unicorn/recommended"
    ],
    "settings": {
      "import/resolver": {
        "node": true,
        "typescript": {
          "moduleDirectory": [
            "src/types"
          ]
        }
      },
      "import/external-module-folders": [
        "node_modules",
        "src/types"
      ]
    },
    "rules": {
      "no-dupe-class-members": "off",
      "no-undef": "off",
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal"
            ],
            [
              "parent",
              "sibling",
              "index"
            ]
          ],
          "newlines-between": "never"
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "printWidth": 100,
          "semi": false,
          "singleQuote": true,
          "trailingComma": "all",
          "arrowParens": "always"
        }
      ],
      "typescript/adjacent-overload-signatures": "error",
      "typescript/class-name-casing": "error",
      "typescript/explicit-member-accessibility": "error",
      "typescript/interface-name-prefix": "error",
      "typescript/member-naming": [
        "error",
        {
          "private": "^_|^constructor$"
        }
      ],
      "typescript/member-ordering": [
        "error",
        {
          "default": [
            "public-static-field",
            "static-field",
            "protected-static-field",
            "private-static-field",
            "public-instance-field",
            "instance-field",
            "protected-instance-field",
            "private-instance-field",
            "public-field",
            "field",
            "protected-field",
            "private-field",
            "public-constructor",
            "constructor",
            "protected-constructor",
            "private-constructor",
            "private-method",
            "private-instance-method",
            "private-static-method",
            "protected-method",
            "protected-instance-method",
            "protected-static-method",
            "public-method",
            "method",
            "public-instance-method",
            "instance-method",
            "public-static-method",
            "static-method"
          ]
        }
      ],
      "typescript/no-angle-bracket-type-assertion": "error",
      "typescript/no-namespace": [
        "error",
        {
          "allowDeclarations": true
        }
      ],
      "typescript/no-parameter-properties": "error",
      "typescript/no-triple-slash-reference": "error",
      "typescript/no-unused-vars": "error",
      "typescript/no-use-before-define": "error",
      "typescript/type-annotation-spacing": "error",
      "unicorn/custom-error-definition": "warn",
      "unicorn/import-index": "off"
    },
    "overrides": [
      {
        "files": [
          "*.test.ts"
        ],
        "rules": {
          "prettier/prettier": [
            "error",
            {
              "printWidth": 100,
              "semi": false,
              "singleQuote": true,
              "trailingComma": "all"
            }
          ]
        }
      }
    ]
  },
  "eslintIgnore": [
    "lib/",
    "coverage/",
    "test/fixtures/",
    "test/snapshots/"
  ]
}
